#include <stdio.h>
int main()
{
    int a =5;
    int*x =&a;   //int*  --> int ka address store karta hai 
        // vvip   *x=7;   // a is changed 
    int **y=&x;   // int** -->  int* ka address store karta hai 
    // ** double pointer 
    printf("%p\n",&a);
   printf("%p\n",&x);     
   printf("%p\n",**&y);
    return 0;
}  
